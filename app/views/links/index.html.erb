<h1>url link</h1>

<%= link_to 'New Link', new_link_path %>
<br>
<table>
  <thead>
    <tr>
      <th>Originurl</th>
      <th>Slug</th>
      <th>Click</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @links.each do |link| %>
      <tr>
        <td><%= link.originurl %></td>
        <td><%= link.slug %></td>
        <td><%= link.click %></td>
        <td><%= link_to 'Show', link %></td>
        <td><%= link_to 'Edit', edit_link_path(link) %></td>
        <td><%= link_to 'Destroy', link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>